{
  "name": "components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "ts-node server",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint-fix": "npx eslint --ext js,jsx,ts,tsx . --fix",
    "test:vi": "vitest run --coverage",
    "test:clean": "npx rimraf coverage && npx rimraf .nyc_output",
    "cy:open": "cypress open --e2e --browser chrome",
    "cy:run": "cypress run --e2e",
    "cy": "start-server-and-test start http://localhost:8000 cypress:run",
    "test:cy": "npx start-server-and-test start http-get://localhost:8000 cy:run && npx nyc report && npx nyc report --reporter=text-summary",
    "test": "npm run test:vi && npm run test:cy",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-redux": "^8.0.5",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.8.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.2",
    "babel-plugin-istanbul": "^6.1.1",
    "cross-fetch": "^3.1.5",
    "cypress": "^12.10.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "msw": "^1.2.1",
    "nyc": "^15.1.0",
    "prettier": "2.8.2",
    "redux-typescript": "^1.2.1",
    "sass": "^1.59.3",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.1.0",
    "vite-plugin-checker": "^0.5.6",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.29.2"
  }
}
